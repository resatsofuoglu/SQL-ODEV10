SELECT city.city, country.country FROM city
LEFT JOIN country ON city.city_id = country.country_id;   1.)

SELECT customer.first_name, customer.last_name, payment.payment_id FROM payment
RIGHT JOIN customer ON customer.customer_id = payment.payment_id;   2.)

SELECT customer.first_name, customer.last_name, rental.rental_id FROM customer
FULL JOIN rental ON customer.customer_id = rental.rental_id;  3.)
